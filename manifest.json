{
  "name": "No Feed",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Hide the Facebook News Feed",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
  "content_scripts": [
    {
      "matches": [
        "http://www.facebook.com/",
        "https://www.facebook.com/",
        "http://www.facebook.com/home.php",
        "https://www.facebook.com/home.php"
      ],
      "js": ["nofeed.js"],
      "run_at": "document_end"
    }
  ]
}
