{
  "name": "No Feed",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Hide the Facebook News Feed",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.facebook.com/",
        "https://www.facebook.com/"
      ],
      "js": ["nofeed.js"],
      "run_at": "document_end"
    }
  ]
}
